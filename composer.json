{
    "name": "revolution/laravel-openbd",
    "description": "OpenBD API",
    "keywords": [
        "openbd",
        "laravel"
    ],
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\OpenBD\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\OpenBD\\Providers\\OpenBDServiceProvider"
            ],
            "aliases": {
                "OpenBD": "Revolution\\OpenBD\\Facades\\OpenBD"
            }
        }
    }
}
